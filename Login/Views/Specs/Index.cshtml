@model IEnumerable<login2.Models.Spec>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Intvalue)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Stringvalue)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Product)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Intvalue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stringvalue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Product.ProductId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SpecId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SpecId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SpecId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
